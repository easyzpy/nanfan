<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.randing.system.mapper.UnitFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.randing.system.domain.po.UnitFile">
        <result column="id" property="id" />
        <result column="unit_file_id" property="unitFileId" />
        <result column="unit_id" property="unitId" />
        <result column="file_url" property="fileUrl" />
        <result column="file_name" property="fileName" />
        <result column="file_type" property="fileType" />
        <result column="create_date" property="createDate" />
        <result column="create_user" property="createUser" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        unit_file_id, unit_id, file_url, file_name, file_type, create_date, create_user
    </sql>

</mapper>
